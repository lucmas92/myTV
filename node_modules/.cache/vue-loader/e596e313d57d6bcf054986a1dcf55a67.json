ms-center justify-center w-8 h-8 py-0 border rounded-md shadow hover:bg-gray-700\">\n        <svg viewBox=\"0 0 24 24\" stroke=\"currentColor\" stroke-width=\"2\" fill=\"none\" stroke-linecap=\"round\"\n             stroke-linejoin=\"round\" class=\"w-4\">\n          <polyline points=\"9 18 15 12 9 6\"></polyline>\n        </svg>\n      </button>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'Pagination',\n  props: {\n    n_elements: Number,\n  },\n  data: function () {\n    return {\n      current_page: 1,\n      max_page: null\n    }\n  },\n  mounted() {\n    this.max_page = Math.ceil(this.n_elements / 10);\n    let page = localStorage.getItem('current_page');\n    if (page == null) {\n      page = 1;\n      localStorage.setItem('current_page', 1);\n    }\n    this.current_page = page\n  },\n  watch: {\n    current_page: function (val) {\n      localStorage.setItem('current_page', val);\n      this.$emit('page_changed')\n    },\n  },\n  methods: {\n    next_page() {\n      if (this.current_page < this.max_page) {\n        this.current_page = parseInt(this.current_page) + 1;\n      }\n    },\n    prev_page() {\n      if (this.current_page > 1) {\n        this.current_page = parseInt(this.current_page) - 1;\n      }\n    },\n  }\n}\n</script>"]}]}